{
  "name": "@cbuschka/flux",
  "version": "1.1.3",
  "description": "Simple, promisified, event emitter based, flux dispatcher.",
  "author": "cbuschka@gmail.com",
  "homepage": "https://github.com/cbuschka/flux",
  "repository": "cbuschka/flux",
  "main": "dist/legacy/index.js",
  "module": "src/index.js",
  "scripts": {
    "clean": "rimraf dist && mkdirp dist/legacy",
    "build:legacy": "BABEL_ENV=legacy babel src --source-maps --no-comments --out-dir dist/legacy/",
    "build": "npm run clean && run-p build:**",
    "release": "npm run build && npm publish --access public"
  },
  "keywords": [
    "simple",
    "es6",
    "oo",
    "event",
    "flux",
    "dispatcher"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
  },
  "peerDdependencies": {
    "babel-runtime": "^6.26.0"
  },
  "babel": {
    "env": {
      "legacy": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": 8,
                "browsers": [
                  "ie >= 11"
                ]
              },
              "spec": true,
              "modules": "commonjs",
              "include": [
                "transform-es2015-classes"
              ],
              "useBuiltIns": "usage"
            }
          ]
        ],
        "plugins": [
          [
            "transform-runtime",
            {
              "helpers": true,
              "polyfill": false,
              "regenerator": true,
              "moduleName": "babel-runtime"
            }
          ]
        ]
      }
    }
  }
}
