{
  "env": {
    "legacy": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": 8,
              "browsers": ["ie >= 11"]
            },
            "spec": true,
            "modules": "commonjs",
            "useBuiltIns": "usage"
          }
        ]
      ],
      "plugins": [
        [
          "@babel/plugin-transform-runtime", {
            "helpers": true,
            "regenerator": true
          }
        ]
      ]
    }
  }
}
